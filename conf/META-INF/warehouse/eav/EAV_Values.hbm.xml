<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="warehouse.models.eav.EAV_Boolean" table="eav_entity_boolean" schema="warehouse">
		<id name="id" type="long" column="value_id">
			<generator class="identity" />
		</id>
		
		<property name="value">
			<column name="value"/>
		</property>
		
		<property name="attributeId">
			<column name="attribute_id"/>
		</property>
		
		<many-to-one name="attribute" class="warehouse.models.eav.EAV_Attribute" update="false" insert="false">
			<column name="attribute_id"></column>
		</many-to-one>

        <property name="entityId">
            <column name="entity_id"/>
        </property>

        <many-to-one name="entity" class="warehouse.models.eav.EAV_Entity" update="false" insert="false">
            <column name="entity_id"></column>
        </many-to-one>
		
	</class>

