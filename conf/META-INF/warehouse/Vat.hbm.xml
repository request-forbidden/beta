<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="warehouse.models.Vat" table="wh_vat" schema="warehouse">
		<id name="id" type="int" column="vat_id">
			<generator class="identity" />
		</id>
		
		<property name="value">
			<column name="value"/>
		</property>
	</class>
</hibernate-mapping>