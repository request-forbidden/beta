<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="warehouse.models.RequestEntityType" table="wh_request_entity_types" schema="warehouse">
		<id name="id" type="long" column="request_entity_type_id">
			<generator class="identity" />
		</id>

        <property name="entityTypeId" column="entity_type_id"></property>

        <property name="requestId" column="request_id"></property>

        <property name="quantity" column="quantity"></property>

        <property name="accepted" column="is_accepted"></property>

        <property name="description" column="description"></property>

        <property name="active" column="active"></property>

        <many-to-one name="entityType" class="warehouse.models.EntityType" update="false" insert="false">
            <column name="entity_type_id"></column>
        </many-to-one>

        <many-to-one name="request" class="warehouse.models.Request" update="false" insert="false">
            <column name="request_id"></column>
        </many-to-one>

	</class>
</hibernate-mapping>