<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="warehouse.models.SupplierContact" table="wh_supplier_contact" schema="warehouse">

		<id name="id" type="long" column="id">
			<generator class="identity" />
		</id>

		<property name="supplierId">
			<column name="supplier_id"/>
		</property>

		<property name="role">
			<column name="role"/>
		</property>

		<property name="firstname">
			<column name="firstname"/>
		</property>

		<property name="lastname">
			<column name="lastname"/>
		</property>

		<property name="phone1">
			<column name="phone1"/>
		</property>

		<property name="phone2">
			<column name="phone2"/>
		</property>

		<property name="email">
			<column name="email"/>
		</property>

		<property name="info">
			<column name="info"/>
		</property>

	</class>
</hibernate-mapping>