<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="warehouse.models.Supplier" table="wh_supplier" schema="warehouse">

		<id name="id" type="long" column="id">
			<generator class="identity" />
		</id>

		<property name="name">
			<column name="name"/>
		</property>

		<property name="fullname">
			<column name="fullname"/>
		</property>

<!--
		<property name="recipient">
			<column name="recipient"/>
		</property>
-->

		<property name="nip">
			<column name="nip"/>
		</property>

		<property name="regon">
			<column name="regon"/>
		</property>

		<property name="email">
			<column name="email"/>
		</property>

		<property name="httpaddress">
			<column name="httpaddress"/>
		</property>

        <!--
		<property name="addressId">
			<column name="address_id"/>
		</property>
		-->

		<property name="city">
			<column name="city"/>
		</property>

		<property name="postcode">
			<column name="postcode"/>
		</property>

		<property name="street">
			<column name="street"/>
		</property>

	<!--

		<many-to-one name="address" class="common.models.Address" not-null="true" update="false" insert="false"> 
			<column name="address_id"></column>
		</many-to-one>
		
	-->
		

	</class>
</hibernate-mapping>